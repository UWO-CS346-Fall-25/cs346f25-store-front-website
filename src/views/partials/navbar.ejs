<header class="site-header" role="banner">
  <div class="container nav-inner">
    <a href="/" class="brand" aria-label="Home">
      <!-- Remove width/height attributes; control via CSS so it won't clip -->
      <img src="/images/RavensTreasures_Logo.png" alt="Raven's Treasures logo" decoding="async" loading="eager">
      <span class="brand-title"><%- siteName %></span>
    </a>

    <nav id="primary-nav" class="nav" aria-label="Primary">
      <ul class="nav-list" role="list">
        <%- include('navbar-link', { path: '/' , link_title: 'Home' }) %>
        <%- include('navbar-link', { path: '/contact' , link_title: 'Contact' }) %>

        <% if (isAdmin) { %>
          <%- include('navbar-link', { path: '/auth/admin' , link_title: 'Admin' }) %>
        <% } %>
        <% if (false) { %>
          <%- include('navbar-link', { path: '/shop/cart' , link_title: 'Cart' + (cartCount> 0 ? ' (' +
            cartCount + ')' : '') }) %>
        <% } %>

        <% if (user !=null) { %>
          <%- include('navbar-link', { path: '/account' , link_title: 'Account' }) %>
          <%- include('navbar-link', { path: '/logout' , link_title: 'Logout' }) %>
        <% } else { %>
          <%- include('navbar-link', { path: '/login' , link_title: 'Login' }) %>
          <%- include('navbar-link', { path: '/signup' , link_title: 'Sign up' }) %>
        <% } %>
      </ul>
    </nav>
  </div>
</header>